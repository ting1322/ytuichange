{
  "manifest_version": 3,
  "name": "yt UI change",
  "description": "change youtube UI",
  "version": "0.0.3",
  "icons": {
    "64": "icons/icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/watch?v=*",
        "https://www.youtube.com/live/*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ]
}